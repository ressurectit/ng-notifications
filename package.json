{
    "name": "@anglr/notifications",
    "version": "0.0.0-placeholder",
    "description": "Angular module for displaying notifications",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run ngc",
        "postbuild": "copyfiles -u 2 dist/src/**/*.* dist && copyfiles -u 2 dist/testing/**/*.* testing && rimraf dist/src dist/dist dist/node_modules dist/testing",
        "clean": "rimraf dist testing tests docs",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run version:placeholder && npm run clean && npm run docs",
        "bundle": "npm run clean && npm run build && copyfiles -u 1 src/**/*.html dist && nnrh *.js -d dist",
        "deploy": "npm publish",
        "deploy:local": "npm publish --registry http://local:4873",
        "deploy:next": "npm publish --tag next",
        "deploy:next:local": "npm publish --tag next --registry http://local:4873",
        "docs": "compodoc src -d docs -n '@anglr/notifications' -p tsconfig.json --disablePrivate --disableInternal --disableLifeCycleHooks",
        "postdocs": "copyfiles -u 1 docs/**/*.* \\\\DOCS-SERVER\\docs\\ng-notifications && npm run clean",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "keywords": [
        "angular",
        "ng",
        "notifications"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "peerDependencies": {
        "@angular/core": ">=6.0.0",
        "@angular/common": ">=6.0.0",
        "@angular/animations": ">=6.0.0",
        "rxjs": ">=6.0.0",
        "@jscrpt/common": ">=1.0.0",
        "@anglr/animations": ">=5.0.0"
    },
    "devDependencies": {
        "@angular/core": "^6.0.0",
        "@angular/common": "^6.0.0",
        "@angular/animations": "^6.0.0",
        "rxjs": "^6.0.0",
        "@jscrpt/common": "^1.0.0",
        "@anglr/animations": "^5.0.0",
        "@types/jasmine": "2.8.7",
        "@types/node": "10.1.2",
        "@angular/compiler-cli": "^6.0.0",
        "@angular/compiler": "^6.0.0",
        "@angular/language-service": "^6.0.0",
        "@angular/platform-browser-dynamic": "^6.0.0",
        "@angular/platform-browser": "^6.0.0",
        "phantomjs-prebuilt": "2.1.16",
        "karma-cli": "1.0.1",
        "karma": "2.0.2",
        "karma-jasmine": "1.1.2",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-webpack": "3.0.0",
        "karma-jasmine-html-reporter": "1.1.0",
        "jasmine": "3.1.0",
        "awesome-typescript-loader": "5.0.0",
        "angular2-template-loader": "0.6.2",
        "raw-loader": "0.5.1",
        "sass-loader": "7.0.1",
        "css-loader": "0.28.11",
        "file-loader": "1.1.11",
        "style-loader": "0.21.0",
        "@compodoc/compodoc": "1.1.3",
        "rimraf": "2.6.2",
        "copyfiles": "2.0.0",
        "replace-in-file": "3.4.0",
        "typescript": "2.7.2",
        "webpack": "4.8.1",
        "npm-git-version": "1.1.0",
        "npm-ng-require-html": "1.0.0"
    }
}
